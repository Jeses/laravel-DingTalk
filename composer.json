{
  "name": "zhengcai/ding-talk-robot",
  "description": "DingTalk robot message handle for send message for laravel project.. ",
  "version": "1.0.0",
  "keywords": ["laravel", "ding talk", "DingTalk", "Notice"],
  "homepage": "https://github.com/Jeses/laravel-DingTalk.git",
  "authors": [
    {
      "name": "Zhengcai",
      "email": "yz_luck@163.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Zhengcai\\DingTalk\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zhengcai\\DingTalk\\DingTalkServiceProvider"
      ],
      "aliases": {
        "DingTalk": "Zhengcai\\DingTalk\\Facades\\DingTalk"
      }
    }
  }
}
